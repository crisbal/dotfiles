Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hintstyle:  hintfull

urxvt*loginShell:         true
urxvt*saveLines:         65535
urxvt*urgentOnBell:       true
!urxvt*override-redirect:false

! Scrollbar
URxvt.scrollBar:            false

! Perl extensions
URxvt.perl-ext-common: default,matcher,resize-font,selection-to-clipboard
URxvt.url-launcher: chromium
URxvt.matcher.button: 1

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.iso14755: false
URxvt.iso14755_52: false

!Cursor
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

! Transparency
URxvt.transparent: true
URxvt.shading: 20
URxvt.fading:  30

! X colors.
! Generated by 'wal'
*foreground:        #dad4d1
*background:        #0d0d0a
*.foreground:       #dad4d1
*.background:       #0d0d0a
emacs*foreground:   #dad4d1
emacs*background:   #0d0d0a
URxvt*foreground:   #dad4d1
XTerm*foreground:   #dad4d1
UXTerm*foreground:  #dad4d1
URxvt*background:   [100]#0d0d0a
XTerm*background:   #0d0d0a
UXTerm*background:  #0d0d0a
URxvt*cursorColor:  #dad4d1
XTerm*cursorColor:  #dad4d1
UXTerm*cursorColor: #dad4d1
URxvt*borderColor:  [100]#0d0d0a

! Colors 0-15.
*.color0: #0d0d0a
*color0:  #0d0d0a
*.color1: #6D6C71
*color1:  #6D6C71
*.color2: #897973
*color2:  #897973
*.color3: #7B7D82
*color3:  #7B7D82
*.color4: #7E8186
*color4:  #7E8186
*.color5: #8E8E90
*color5:  #8E8E90
*.color6: #ADA6A3
*color6:  #ADA6A3
*.color7: #dad4d1
*color7:  #dad4d1
*.color8: #989492
*color8:  #989492
*.color9: #6D6C71
*color9:  #6D6C71
*.color10: #897973
*color10:  #897973
*.color11: #7B7D82
*color11:  #7B7D82
*.color12: #7E8186
*color12:  #7E8186
*.color13: #8E8E90
*color13:  #8E8E90
*.color14: #ADA6A3
*color14:  #ADA6A3
*.color15: #dad4d1
*color15:  #dad4d1

! Black color that will not be affected by bold highlighting.
*.color66: #0d0d0a
*color66:  #0d0d0a

! Set depth to make transparency work.
URxvt*depth: 32

! Border
URxvt.internalBorder: 10

! Font
URxvt.font: xft:Hack:size=14
URxvt.letterSpace: 0
URxvt.lineSpace: 0

! do not scroll with output
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

